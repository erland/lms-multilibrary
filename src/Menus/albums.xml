<?xml version="1.0" encoding="utf-8"?>
<custombrowse>
	<template>
		<id>albums.xml</id>
		<parameter type="text" id="menuname"><value>Albums</value></parameter>
		<parameter type="text" id="menugroup"><value>{libraryname}</value></parameter>
		<parameter type="text" id="menuorder"><value>50</value></parameter>
		<parameter type="text" id="showartistwithalbum"></parameter>
		<parameter type="text" id="playalltracks"><value>1</value></parameter>
		<parameter type="text" id="library"><value>{libraryno}</value></parameter>
		<parameter type="text" id="includedclients">{includedclients}</parameter>
		<parameter type="text" id="excludedclients">{excludedclients}</parameter>
	</template>
</custombrowse>
